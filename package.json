{
  "name": "tictactoe",
  "version": "0.1.0",
  "description": "The default template project for [nano-react-app](https://github.com/adrianmcli/nano-react-app).",
  "homepage": "http://AnkitBhujeja.github.io/tictacgame",
  "scripts": {
    "start": "parcel index.html",
    "build": "npm run clear && parcel build index.html",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:lint": "npm run format && npm run lint",
    "clear": "rimraf dist .parcel-cache",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "license": "MIT",
  "dependencies": {
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-preset-nano-react-app": "^0.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^3.2.3",
    "parcel": "^1.12.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnkitBhujeja/tictacgame.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/AnkitBhujeja/tictacgame/issues"
  }
}
